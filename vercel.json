{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "npm install && prisma generate && next build",
  "installCommand": "npm install",
  "devCommand": "next dev",
  "crons": [
    {"path": "/api/cron/system-metrics", "schedule": "*/5 * * * *"},
    {"path": "/api/cron/ai-agents", "schedule": "0 * * * *"},
    {"path": "/api/cron/anomaly-detection", "schedule": "*/15 * * * *"},
    {"path": "/api/cron/data-quality", "schedule": "0 2 * * *"},
    {"path": "/api/cron/data-retention", "schedule": "0 2 * * *"},
    {"path": "/api/cron/risk-scoring", "schedule": "0 3 * * *"},
    {"path": "/api/cron/churn-prediction", "schedule": "0 4 * * *"},
    {"path": "/api/cron/analytics-aggregation", "schedule": "0 1 * * *"},
    {"path": "/api/cron/business-intelligence", "schedule": "0 5 * * *"},
    {"path": "/api/cron/backup-check", "schedule": "0 */6 * * *"},
    {"path": "/api/cron/owner-digest", "schedule": "0 8 * * *"},
    {"path": "/api/cron/cofounder-report", "schedule": "0 0 * * *"},
    {"path": "/api/cron/cofounder-report", "schedule": "0 8 * * *"},
    {"path": "/api/cron/cofounder-report", "schedule": "0 16 * * *"}
  ]
}
